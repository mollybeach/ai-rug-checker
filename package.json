{
    "name": "rug-watch-dog",
    "version": "1.0.0",
    "description": "AI-powered cryptocurrency token analyzer for detecting potential rug pulls",
    "main": "dist/api/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/api/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/api/server.ts",
        "watch": "tsc --watch",
        "scan": "ts-node-dev --transpile-only src/scripts/scan.ts",
        "train": "ts-node-dev --transpile-only src/scripts/train.ts",
        "collect-data": "ts-node-dev --transpile-only src/scripts/collect-data.ts",
        "clean-db": "ts-node-dev --transpile-only src/scripts/clean-db.ts",
        "init-db": "ts-node src/scripts/init-db.ts",
        "seed-db": "ts-node src/scripts/seed-db.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "migrate": "sequelize-cli db:migrate",
        "migrate:undo": "sequelize-cli db:migrate:undo",
        "seed": "sequelize-cli db:seed:all",
        "prepare": "npm run build",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@tensorflow/tfjs-node": "^4.11.0",
        "@types/sequelize": "^4.28.20",
        "axios": "^1.5.0",
        "dotenv": "^16.3.1",
        "ethers": "^6.7.1",
        "express": "^4.18.2",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "sequelize": "^6.37.5",
        "sqlite3": "^5.1.6",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.17.12",
        "@types/pg": "^8.11.10",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "eslint": "^8.49.0",
        "jest": "^29.7.0",
        "sequelize-cli": "^6.6.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/rug-watch-dog.git"
    },
    "keywords": [
        "cryptocurrency",
        "blockchain",
        "machine-learning",
        "security",
        "ethereum"
    ],
    "author": "Your Name",
    "license": "MIT"
}