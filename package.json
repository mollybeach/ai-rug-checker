{
    "name": "rug-watch-dog",
    "version": "0.1.1",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "build": "tsc && tsup src/index.ts --format esm --dts",
        "start": "cross-env NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
        "train": "cross-env NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm scripts/trainModel.ts",
        "collect-data": "cross-env NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm scripts/collectData.ts",
        "test": "jest --passWithNoTests",
        "dev": "nodemon --watch src --ext ts,json --exec 'cross-env NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts'",
        "clean": "rm -rf dist && rm -rf node_modules",
        "scan": "node --loader ts-node/esm src/scan.ts"
    },
    "dependencies": {
        "@elizaos/adapter-postgres": "0.1.7",
        "@elizaos/adapter-sqlite": "0.1.7",
        "@elizaos/client-auto": "0.1.7",
        "@elizaos/client-direct": "0.1.7",
        "@elizaos/client-discord": "0.1.7",
        "@elizaos/client-telegram": "0.1.7",
        "@elizaos/client-twitter": "0.1.7",
        "@elizaos/core": "0.1.7",
        "@elizaos/plugin-bootstrap": "0.1.7",
        "@elizaos/plugin-image-generation": "0.1.7",
        "@elizaos/plugin-node": "0.1.7",
        "@elizaos/plugin-solana": "0.1.7",
        "@elizaos/plugin-starknet": "0.1.7",
        "@tavily/core": "0.0.2",
        "@tensorflow/tfjs-core": "^4.22.0",
        "@tensorflow/tfjs-node": "4.22.0",
        "@types/axios": "^0.14.4",
        "amqplib": "0.10.5",
        "axios": "^1.7.9",
        "better-sqlite3": "11.5.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.7",
        "ethers": "^6.13.5",
        "express": "^4.21.2",
        "fs": "0.0.1-security",
        "net": "1.0.2",
        "path": "0.12.7",
        "pdfjs-dist": "3.11.174",
        "readline": "1.3.0",
        "url": "0.11.4",
        "ws": "8.18.0",
        "yargs": "17.7.2"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "pnpm": {
        "overrides": {
            "onnxruntime-node": "1.20.0"
        }
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@jest/types": "^29.6.3",
        "@types/better-sqlite3": "^7.6.12",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.5",
        "@types/yargs": "^17.0.33",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "ts-node": "10.9.2",
        "tsup": "8.3.5",
        "typescript": "5.6.3"
    }
}